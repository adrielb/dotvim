
snippet     frac
alias       \frac
	\frac{ ${1:#:numerator} }{ ${2:#:denomerator} }


snippet figure
alias   \begin{figure} \figure
	\begin{figure}
		\centering
		\includegraphics[width=0.9\textwidth]{${1}}
		\caption{
            ${2} 
        } \label{fig:$1}
	\end{figure}

snippet tikzpicture
alias   \begin{tikzpicture} \tikzpicture
	\begin{tikzpicture}
        \draw[style=help lines] (0,0) grid (4,3);
		${1:TARGET}
	\end{tikzpicture}

